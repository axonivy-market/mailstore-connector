  "kind" : "CALLABLE_SUB",
  "config" : {
    "data" : "com.axonivy.market.mailstore.connector.demo.MessageHandlerOverrideData"
  },
  "elements" : [ {
      "id" : "f6",
      "type" : "CallSubStart",
      "name" : "handleMessage(Message)",
      "config" : {
        "callSignature" : "handleMessage",
        "input" : {
          "params" : [
            { "name" : "message", "type" : "javax.mail.Message" }
          ],
          "map" : {
            "out.message" : "param.message"
          }
        },
        "result" : {
          "params" : [
            { "name" : "handled", "type" : "Boolean", "desc" : "Was the mail handled (and can be re/moved)?" }
          ],
          "map" : {
            "result.handled" : "in.handled"
          }
        }
      },
      "visual" : {
        "at" : { "x" : 104, "y" : 64 },
        "labelOffset" : { "x" : 17, "y" : -23 }
      },
      "connect" : { "id" : "f9", "to" : "f8" }
    }, {
      "id" : "f7",
      "type" : "CallSubEnd",
      "visual" : {
        "at" : { "x" : 104, "y" : 224 }
      }
    }, {
      "id" : "f8",
      "type" : "Script",
      "name" : [
        "Handle single",
        "message"
      ],
      "config" : {
        "output" : {
          "code" : [
            "import javax.mail.Part;",
            "import java.util.Collection;",
            "import com.axonivy.market.mailstore.connector.MessageService;",
            "",
            "ivy.log.info(\"Working on message {0} received at {1}\", in.message.subject, in.message.getReceivedDate());",
            "",
            "Collection<Part> parts = MessageService.getAllParts(in.message, false, MessageService.isImage(\"*\"));",
            "",
            "if(parts.size() == 0) {",
            "  ivy.log.info(\"Message does not contain any images\");",
            "}",
            "else {",
            "  for (Part part : parts) {",
            "    ivy.log.info(\"Found image: Filename: {0} Description: {1} ContentType: {2} Disposition: {3} Content Class: {4}\",",
            "            part.fileName, part.description, part.getContentType(), part.disposition, part.getContent().getClass());",
            "  }",
            "}",
            "",
            "in.handled = true;"
          ]
        }
      },
      "visual" : {
        "at" : { "x" : 104, "y" : 144 }
      },
      "connect" : { "id" : "f10", "to" : "f7" }
    } ]
}